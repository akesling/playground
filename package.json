{
  "name": "artichoke-playground",
  "version": "0.1.0",
  "private": true,
  "description": "Artichoke Ruby Wasm Playground",
  "keywords": [
    "programming language",
    "scripting",
    "ruby",
    "rust",
    "wasm"
  ],
  "homepage": "https://www.artichoke.run",
  "bugs": "https://github.com/artichoke/playground/issues",
  "license": "MIT",
  "author": {
    "name": "Ryan Lopopolo",
    "email": "rjl@hyperbo.la",
    "url": "https://hyperbo.la"
  },
  "contributors": [
    {
      "name": "Ryan Lopopolo",
      "email": "rjl@hyperbo.la",
      "url": "https://hyperbo.la"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/artichoke/playground.git"
  },
  "dependencies": {
    "ace-builds": "^1.4.5",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
    "@artichoke/logo": "git://github.com/artichoke/logo#master",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clang-format": "^1.2.4",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "package-json-validator": "^0.6.3",
    "pkg-dir-cli": "^2.1.0",
    "prettier": "^1.15.3",
    "raw-loader": "^4.0.0",
    "script-loader": "^0.7.2",
    "style-loader": "^1.0.1",
    "svg-url-loader": "^3.0.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^2.2.2",
    "uglify-loader": "^3.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "html",
      "import",
      "prettier"
    ],
    "extends": [
      "airbnb",
      "prettier",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "globals": {
      "_artichoke_web_repl_init": "readonly",
      "_artichoke_build_info": "readonly",
      "_artichoke_playground_eval": "readonly"
    },
    "rules": {
      "no-underscore-dangle": "off",
      "prettier/prettier": "error",
      "react/jsx-closing-bracket-location": "off",
      "react/jsx-closing-tag-location": "off",
      "react/jsx-curly-newline": "off",
      "react/jsx-curly-spacing": "off",
      "react/jsx-equals-spacing": "off",
      "react/jsx-first-prop-new-line": "off",
      "react/jsx-indent": "off",
      "react/jsx-indent-props": "off",
      "react/jsx-max-props-per-line": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-props-no-multi-spaces": "off",
      "react/jsx-tag-spacing": "off",
      "react/jsx-wrap-multilines": "off",
      "react/self-closing-comp": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {},
        "webpack": {}
      }
    }
  },
  "scripts": {
    "eslint-check": "eslint --print-config package.json | eslint-config-prettier-check",
    "lint": "./scripts/lint.sh",
    "serve-wasm": "webpack-dev-server --mode production --open"
  }
}
